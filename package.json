{
  "name": "one-shot",
  "version": "1.0.0",
  "description": "Flooki (formerly one-shot)",
  "main": "index.js",
  "scripts": {
      "start": "budo src -d --serve bundle.js --live",
      "build": "browserify src -o bundle.js",
      "test": "browserify test/index.js | node | tap-spec",
      "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
      "icons": "bash -c 'cp -r icons dist'",
      "push": "gh-pages -d dist",
      "deploy": "npm run build && npm run dist && npm run icons && npm run push"
    },
    "browserify": {
      "transform": ["babelify"]
    },
  "repository": {
    "type": "git",
    "url": "https://github.com/one-mile/client"
  },
  "author": "one-mile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/one-mile/client/issues"
  },
  "homepage": "https://github.com/one-mile/client",
  "dependencies": {
    "babelify": "^7.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.1.1",
    "budo": "^9.2.2",
    "clone": "^2.1.0",
    "cloudinary": "^1.4.6",
    "cloudinary-core": "^2.1.7",
    "deep-freeze": "0.0.1",
    "gh-pages": "^0.12.0",
    "moment": "^2.17.1",
    "morphdom": "^2.2.1",
    "on-load": "^3.2.0",
    "redux": "^3.6.0",
    "sheet-router": "^4.1.2",
    "superagent": "^3.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "uglifyify": "^3.0.4",
    "yo-yo": "^1.3.1"
  }
}
