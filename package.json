{
  "name": "one-shot",
  "version": "1.0.0",
  "description": "Flooki (formerly one-shot)",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify src -o build/bundle.js",
    "clean": "rm -rf build/*",
    "dist": "bash -c 'cp *.{html,css} build'",
    "test": "browserify test/index.js | node | tap-spec",
    "push": "gh-pages -d build",
    "deploy": "npm run clean && npm run build && npm run dist && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/one-mile/client"
  },
  "author": "one-mile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/one-mile/client/issues"
  },
  "homepage": "https://github.com/one-mile/client",
  "dependencies": {
    "browserify": "^13.1.1",
    "budo": "^9.2.2",
    "clone": "^2.1.0",
    "cloudinary": "^1.4.6",
    "cloudinary-core": "^2.1.7",
    "deep-freeze": "0.0.1",
    "gh-pages": "^0.12.0",
    "moment": "^2.17.1",
    "morphdom": "^2.2.1",
    "on-load": "^3.2.0",
    "redux": "^3.6.0",
    "sheet-router": "^4.1.2",
    "superagent": "^3.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "uglifyify": "^3.0.4",
    "yo-yo": "^1.3.1"
  }
}
